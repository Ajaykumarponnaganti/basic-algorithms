def is_armstrong(n):
    sum = 0
    temp = n
    while temp > 0:
        digit = temp % 10  t
        sum += digit ** 3   
        temp //= 10         
    return sum == n
n = int(input("Enter a number: "))
if is_armstrong(n):
    print(f"{n} is an Armstrong number.")
else:
    print(f"{n} is not an Armstrong number.")
